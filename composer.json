{
    "name": "ztsu/reacon",
    "type": "library",
    "description": "PSR-7 middleware runner compatible with PSR-15",
    "keywords": [
        "psr-7", "psr-15", "http", "middleware"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Golovnya",
            "email": "alexey.golovnya@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "ztsu/pipe": "^1.0"
    },
    "require-dev": {
        "zendframework/zend-diactoros": "^1.3",
        "phpunit/phpunit": "^5.6",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "provide": {
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ztsu\\Reacon\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit tests/",
        "coverage": "phpunit tests/ --coverage-clover clover.xml"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
