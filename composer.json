{
    "name": "ztsu/reacon",
    "type": "library",
    "description": "PSR-7 middleware runner compatible with PSR-15",
    "keywords": [
        "psr-7", "psr-15", "http", "middleware"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Golovnya",
            "email": "alexey.golovnya@gmail.com"
        }
    ],
    "require": {
        "psr/http-message": "^1.0",
        "ztsu/pipe": "^1.0",
        "http-interop/http-middleware": "^0.4.1"
    },
    "require-dev": {
        "zendframework/zend-diactoros": "^1.3",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Ztsu\\Reacon\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit tests/"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
